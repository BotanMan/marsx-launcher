{
  "name": "@marsx-dev/launcher",
  "version": "0.0.1",
  "description": "MarsX launcher",
  "author": "MarsX team <team@marsx.dev>",
  "homepage": "https://marsx.dev/",
  "license": "Apache-2.0",
  "repository": "github:marsx/launcher",
  "keywords": [
    "marsx",
    "lowcode",
    "nocode",
    "editor",
    "ide"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "marsx": "./dist/cli/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "validate": "npm run validate:prettier && npm run validate:lint && npm run validate:tsc",
    "validate:prettier": "prettier --ignore-path=.gitignore --list-different \"**/*.+(js|ts|json)\"",
    "validate:lint": "eslint --ignore-path=.gitignore --ext=.js,.ts .",
    "validate:tsc": "tsc --noEmit",
    "fix": "npm run fix:prettier && npm run fix:list",
    "fix:prettier": "prettier --ignore-path=.gitignore --write \"**/*.+(js|ts|json)\"",
    "fix:list": "eslint --ignore-path=.gitignore --ext=.js,.ts --fix .",
    "build": "tsc"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.2.29",
    "axios": "^0.26.1",
    "chalk": "^4.1.2",
    "commander": "^9.1.0",
    "config": "^3.3.7",
    "json-stable-stringify": "^1.0.1",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.3.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@fastify/session": "^6.4.0",
    "@stitches/core": "^1.2.6",
    "@types/accepts": "^1.3.5",
    "@types/bcrypt": "^5.0.0",
    "@types/mkdirp": "^1.0.2",
    "@types/nodegit": "^0.27.9",
    "@types/sharp": "^0.29.5",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.2",
    "aws-sdk": "^2.1073.0",
    "azure-storage": "^2.10.7",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "base-x": "^4.0.0",
    "bcrypt": "^5.0.1",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.2",
    "fastify-accepts": "^2.1.0",
    "fastify-compress": "^4.0.1",
    "fastify-cookie": "^5.6.0",
    "fastify-cors": "^6.0.3",
    "fastify-csrf": "^3.1.0",
    "fastify-flash": "^2.0.2",
    "fastify-formbody": "^5.2.0",
    "fastify-helmet": "^7.0.1",
    "fastify-multipart": "^5.3.1",
    "fastify-request-context": "^2.2.0",
    "fastify-static": "^4.5.0",
    "fastify-websocket": "^4.2.0",
    "fontverter": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mobile-detect": "^1.4.5",
    "moment-timezone": "^0.5.34",
    "nodegit": "^0.28.0-alpha.10",
    "route-pattern": "^0.0.6",
    "route-sort": "^1.0.0",
    "sharp": "^0.30.1",
    "source-map": "^0.7.3",
    "subset-font": "^1.4.0",
    "uuid": "^8.3.2",
    "xxhash": "^0.3.0"
  },
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "prettier": {
    "printWidth": 140,
    "tabWidth": 2,
    "endOfLine": "lf",
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  }
}
